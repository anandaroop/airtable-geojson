{
  "name": "airtable-geojson",
  "version": "0.2.0",
  "description": "Convert an Airtable table containing a Map App (aka Map Block) geocoded field into a GeoJSON FeatureCollection",
  "main": "dist/index.js",
  "repository": "https://github.com/anandaroop/airtable-geojson",
  "author": "@anandaroop",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/airtable": "^0.8.1",
    "@types/geojson": "^7946.0.7",
    "@types/jest": "^26.0.14",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "husky": ">=4",
    "jest": "^26.4.2",
    "lint-staged": ">=10",
    "np": "^6.5.0",
    "prettier": "2.1.2",
    "ts-jest": "^26.4.0",
    "typescript": "^4.0.3"
  },
  "scripts": {
    "clean": "rm -rf dist/*",
    "compile": "tsc",
    "watch": "tsc --watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint \"src/**/*.ts\"",
    "test": "jest",
    "prepublishOnly": "yarn clean && yarn compile"
  },
  "files": [
    "dist/**/*"
  ]
}
